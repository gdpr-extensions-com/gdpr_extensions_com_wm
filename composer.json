{
    "name": "gdpr-extensions-com/gdpr_extensions_com_wm",
    "type": "typo3-cms-extension",
    "description": "Manage your websites and add various content elements that are GDPR compliant to your website, GDPR Website Manager is your first step to ensure your userâ€™s privacy.",
    "version": "1.0.2",
    "authors": [],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^11.5 || ^12.4",
        "typo3/cms-scheduler": "^11.5 || ^12.4"
    },
    "require-dev": {
        "typo3/testing-framework": "^6.9.0"
    },
    "autoload": {
        "psr-4": {
            "GdprExtensionsCom\\GdprExtensionsComWm\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GdprExtensionsCom\\GdprExtensionsComWm\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/gdpr-websitemanager": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ],
        "post-install-cmd": [
            "@update-schema"
        ],
        "post-update-cmd": [
            "@update-schema"
        ],
        "update-schema": "typo3cms database:updateschema \"*.add,*.change\""
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "gdpr_extensions_com_wm"
        }
    }
}
